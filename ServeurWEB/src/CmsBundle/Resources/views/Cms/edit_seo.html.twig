<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">SEO</h3>

        <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i>
            </button>
        </div>
        <!-- /.box-tools -->
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        {% block edit_seo %}
            <div class="row">
                {% if form.title is defined %}
                    <div class="col-md-4">{{ form_row(form.title) }}</div>
                {% endif %}
                {% if form.H1 is defined %}
                    <div class="col-md-4">{{ form_row(form.H1) }}</div>
                {% endif %}
                {% if form.slug is defined %}
                    <div class="col-md-4">
                        {{ form_row(form.slug) }}
                        {{ form_row(form.uniqueSlug) }}
                    </div>
                {% endif %}
            </div>
            <div class="row">
                {% if form.metaDescription is defined %}
                    <div class="col-md-4">{{ form_row(form.metaDescription) }}</div>
                {% endif %}
                {% if form.metaKeyword is defined %}
                    <div class="col-md-4">{{ form_row(form.metaKeyword) }}</div>
                {% endif %}
                {% if form.otherMeta is defined %}
                    <div class="col-md-4">{{ form_row(form.otherMeta) }}</div>
                {% endif %}
            </div>
        {% endblock edit_seo %}
    </div>
    <!-- /.box-body -->
</div>